{
	"type": "config",
	"id": "conf.service.staticroutes",
	"title": "Static network routes",
	"queryinfo": {
		"xpath": "//services/staticroutes",
		"iterable": false
	},
	"properties": {
		"route": {
			"type": "array",
			"items": {
				"type": "object",
				"properties": {
					"uuid": {
						"type": "string",
						"format": "uuidv4"
					},
					"network": {
						"type": "string"
					},
					"gateway": {
						"type": "string"
					},
					"dev": {
						"type": "string",
						"format": "devicefile"
					},
					"comment": {
						"type": "string",
						"default": ""
					}
				}
			}
		}
	}
}
