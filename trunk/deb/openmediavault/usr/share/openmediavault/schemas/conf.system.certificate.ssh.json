{
	"type": "config",
	"id": "conf.system.certificate.ssh",
	"alias": "//system/certificates/sshcertificate",
	"title": "SSH certificate",
	"properties": {
		"uuid": {
			"type": "string",
			"format": "uuidv4",
			"default": ""
		},
		"publickey": {
			"type": "string",
			"format": "sshpubkey-openssh",
			"default": ""
		},
		"privatekey": {
			"type": "string",
			"format": "sshprivkey-rsa",
			"default": ""
		},
		"comment": {
			"type": "string",
			"default": ""
		}
	}
}
