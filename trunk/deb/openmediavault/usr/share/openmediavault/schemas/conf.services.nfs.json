{
	"type": "object",
	"id": "conf.service.nfs",
	"alias": "//services/nfs",
	"title": "NFS",
	"properties": {
		"enable": {
			"type": "boolean",
			"default": 0
		},
		"numproc": {
			"type": "integer",
			"default": 8
		},
		"shares": {
			"type": "object",
			"properties": {
				"share": {
					"type": "array",
					"items": {
						"type": "object",
						"properties": {
							"uuid": {
								"type": "string",
								"format": "uuidv4"
							},
							"sharedfolderref": {
								"type": "string",
								"format": "uuidv4"
							},
							"mntentref": {
								"type": "string",
								"format": "uuidv4"
							},
							"client": {
								"type": "string"
							},
							"options": {
								"type": "string"
							},
							"comment": {
								"type": "string"
							},
							"extraoptions": {
								"type": "string"
							}
						}
					}
				}
			}
		}
	}
}
